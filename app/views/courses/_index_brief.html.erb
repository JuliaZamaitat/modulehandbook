
<table class="table table-hover">
  <thead>
    <tr>

      <th scope="col">Name</th>
      <th scope="col">Code</th>
      <th scope="col">Ects</th>
      <th scope="col">Semester</th>
      <th scope="col">required?</th>
      <th scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% course_programs.each do |course_program| %>
      <tr onclick = "location.href='<%= course_program_path%>'">
        <td scope="row"><%= course_program.course.name %></td>
        <td><%= course_program.course.code %></td>
        <td><%= course_program.course.ects %></td>
        <td><%= course_program.semester %></td>
        <td><%= course_program.required %></td>
        <td><%= link_to 'Show', course_program %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<br>
