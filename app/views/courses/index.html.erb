<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Mission</th>
      <th>Ects</th>
      <th>Examination</th>
      <th>Objectives</th>
      <th>Contents</th>
      <th>Prerequisites</th>
      <th>Literature</th>
      <th>Methods</th>
      <th>Skills knowledge understanding</th>
      <th>Skills intellectual</th>
      <th>Skills practical</th>
      <th>Skills general</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.code %></td>
        <td><%= course.mission %></td>
        <td><%= course.ects %></td>
        <td><%= course.examination %></td>
        <td><%= course.objectives %></td>
        <td><%= course.contents %></td>
        <td><%= course.prerequisites %></td>
        <td><%= course.literature %></td>
        <td><%= course.methods %></td>
        <td><%= course.skills_knowledge_understanding %></td>
        <td><%= course.skills_intellectual %></td>
        <td><%= course.skills_practical %></td>
        <td><%= course.skills_general %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
